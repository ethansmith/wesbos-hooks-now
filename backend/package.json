{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect src/index.js",
    "build": "node src/index.js",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env && npm run writeSchema",
    "writeSchema": "node src/writeSchema.js"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "8.0.0",
    "graphql": "^14.4.2",
    "graphql-cli": "^3.0.14",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "1.18.0",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.34.6",
    "prisma-binding": "^2.1.6",
    "stripe": "^7.8.0"
  },
  "license": "MIT",
  "repository": {
    "url": "wesbos.com"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
